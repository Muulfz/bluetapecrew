  <connectionStrings>
    <add name="BtcEntities" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=btcdb;Integrated Security=True;MultipleActiveResultSets=true" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
